{
    "name": "transistor/filament-layout-builder",
    "description": "A drag-and-drop email layout builder for Filament forms",
    "keywords": [
        "filament",
        "email",
        "builder",
        "layout",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Transistor",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/forms": "^3.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Transistor\\FilamentLayoutBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Transistor\\FilamentLayoutBuilder\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Transistor\\FilamentLayoutBuilder\\LayoutBuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
